{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "attach",
            "name": "Attach to remote",
            "address": "localhost",
            "port": 9229,
            "localRoot": "${workspaceFolder}",
            "remoteRoot": "${workspaceFolder}"
        },
        {
            "type": "node-terminal",
            "name": "Run Script: test",
            "request": "launch",
            "command": "npm run test",
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "node-terminal",
            "name": "Test cartesian",
            "request": "launch",
            "command": "yarn jest cartesian",
            "cwd": "${workspaceFolder}",
            "env": {
                "TF_FORCE_GPU_ALLOW_GROWTH": "true"
            },
        }
    ]
}